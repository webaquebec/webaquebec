{
  "name": "webaquebec",
  "version": "1.0.0",
  "private": true,
  "description": "Web à Québec",
  "author": "webaquebec",
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "@csstools/normalize.css": "^11.0.1",
    "@formatjs/cli": "^2.13.15",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-formatjs": "^2.9.10",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-sharp": "^2.13.3",
    "gatsby-plugin-styled-components": "^3.9.0",
    "gatsby-plugin-webfonts": "^1.1.4",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-source-graphql": "^2.13.0",
    "gatsby-transformer-sharp": "^2.11.0",
    "polished": "^4.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "commitizen": "^4.2.2",
    "commitlint-config-gitmoji": "^1.0.1",
    "cz-emoji": "^1.3.1",
    "dumper.js": "^1.3.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.1.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.6.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-high-performance-animation": "^1.5.1",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "npx stylelint \"**/*.{css,jsx,js}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean:modules": "rimraf /node_modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{json,md}": "prettier --write",
    "*.{js,jsx,css}": "stylelint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  }
}
